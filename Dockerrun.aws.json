{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "fastapi",
      "image": "smartspeztech:tag",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["db"],
      "environment": [
        {
          "name": "DATABASE_URL",
          "value": "postgresql://postgres:password@db:5432/mydatabase"
        },
        {
          "name": "ENVIRONMENT",
          "value": "production"
        }
      ]
    },
    {
      "name": "db",
      "image": "postgres:13",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 5432,
          "containerPort": 5432
        }
      ],
      "environment": [
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "password"
        },
        {
          "name": "POSTGRES_DB",
          "value": "mydatabase"
        }
      ]
    }
  ]
}
